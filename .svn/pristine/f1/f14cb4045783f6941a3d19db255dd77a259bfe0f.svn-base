package com.wlg.Model;

import org.hibernate.annotations.GenericGenerator;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import java.io.Serializable;

/**
 * Created by Administrator on 2017/5/10 0010.
 */
@Entity
public class CkStockInfo implements Serializable {


    private String id;
    private String ckid;
    private String gid;
    private String cknum;
    private String ckfnum;
    private String cknnum;
    private String ckdate;
    private String ckperson;
    private String csnum;
    private String csfnum;
    private String csnnum;
    private String cflag;
    private String ckreason;
    private String batchno;
    @Column(length = 36,nullable=true)
    public String getCkreason() {
        return ckreason;
    }

    public void setCkreason(String ckreason) {
        this.ckreason = ckreason;
    }
    @Id
    @GeneratedValue(generator = "system-uuid")
    @GenericGenerator(name = "system-uuid", strategy = "uuid")
    @Column(length = 36)
    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }
    @Column(length = 36,nullable=true)
    public String getCkid() {
        return ckid;
    }

    public void setCkid(String ckid) {
        this.ckid = ckid;
    }
    @Column(length = 36,nullable=true)
    public String getGid() {
        return gid;
    }

    public void setGid(String gid) {
        this.gid = gid;
    }
    @Column(length = 36,nullable=true)
    public String getCknum() {
        return cknum;
    }

    public void setCknum(String cknum) {
        this.cknum = cknum;
    }
    @Column(length = 36,nullable=true)
    public String getCkfnum() {
        return ckfnum;
    }

    public void setCkfnum(String ckfnum) {
        this.ckfnum = ckfnum;
    }
    @Column(length = 36,nullable=true)
    public String getCknnum() {
        return cknnum;
    }

    public void setCknnum(String cknnum) {
        this.cknnum = cknnum;
    }
    @Column(length = 36,nullable=true)
    public String getCkdate() {
        return ckdate;
    }

    public void setCkdate(String ckdate) {
        this.ckdate = ckdate;
    }
    @Column(length = 36,nullable=true)
    public String getCkperson() {
        return ckperson;
    }

    public void setCkperson(String ckperson) {
        this.ckperson = ckperson;
    }
    @Column(length = 36,nullable=true)
    public String getCsnum() {
        return csnum;
    }

    public void setCsnum(String csnum) {
        this.csnum = csnum;
    }
    @Column(length = 36,nullable=true)
    public String getCsfnum() {
        return csfnum;
    }

    public void setCsfnum(String csfnum) {
        this.csfnum = csfnum;
    }
    @Column(length = 36,nullable=true)
    public String getCsnnum() {
        return csnnum;
    }

    public void setCsnnum(String csnnum) {
        this.csnnum = csnnum;
    }
    @Column(length = 36,nullable=true)
    public String getCflag() {
        return cflag;
    }

    public void setCflag(String cflag) {
        this.cflag = cflag;
    }
    @Column(length = 36,nullable=true)
    public String getBatchno() {
        return batchno;
    }

    public void setBatchno(String batchno) {
        this.batchno = batchno;
    }
}
